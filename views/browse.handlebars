<div class="card text-center border-0">
    <div class="card-header border-0">
        Tags
    </div>
    <div class="card-body border-0">
        <p id="hashtags" class="card-text">#dolphins #twix #gimmeabreaeaeaeak</p>
        <form class="myform" action="post">
            <button type="button" class="btn button-up btn-lg" class="vote-btn" data-default-text="fas fa-arrow-circle-up" data-alt-text="up-vote">
                <span class="icon"></span>
                <i class="fas fa-arrow-circle-up"></i>
                <span> Up-vote </span>
                <span class="badge badge-light" onclick="myFunction(hideVote1)" id="vote-count1"> 40 </span>
            </button>

            <button type="button" class="btn button-down btn-lg" class="vote-btn" data-default-text="fas fa-arrow-circle-down" data-alt-text="down-vote">
                <span class="icon"></span>
                <i class="fas fa-arrow-circle-down"></i>
                <span> Down-vote </span>
                <span class="badge badge-light" onclick="myFunction(hideVote2)" id="vote-count2"> 13 </span>
            </button>
        </form>
    </div>
</div>
